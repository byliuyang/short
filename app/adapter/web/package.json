{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "classnames": "^2.2.6",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "material-design-icons": "^3.0.1",
    "qrcode": "^1.4.1",
    "react-github-btn": "^1.0.6",
    "recaptcha-v3": "^1.5.0"
  },
  "scripts": {
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts test",
    "eject": "react-scripts eject",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn prettier:base --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base --write \"src/**/*.{ts,tsx}\"",
    "stylelint:write": " yarn stylelint --config=.stylelintrc --fix src/**/*.{scss,css}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "@types/qrcode": "^1.3.3",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.3",
    "typescript": "^3.5.0"
  }
}
